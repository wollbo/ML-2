function eMu = expectationMu(x,mu0,lambda0,muN,lambdaN)
    
    support = 1:length(x); % represents mu support
    probs = normpdf(support,muN,(1/lambdaN));
    normpdf(mu0, muN, 1/(lambdaN))
    x = conds(50,:);
    I = 0;
    for k = 1:length(support)
        I = probs(k)*sum((x-support)^2+lambda0*(support-mu0)) + I;
    end
    eMu = I;
end
